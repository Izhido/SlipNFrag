                                <TextBlock x:Name="joy_axis_x_text" VerticalAlignment="Center" />
                                <ComboBox x:Name="joy_axis_x_combo" Grid.Column="1" IsTabStop="False" />
                                <CheckBox x:Name="joy_axis_x_check" Grid.Column="2" IsTabStop="False">Inverted</CheckBox>
                            </Grid>
